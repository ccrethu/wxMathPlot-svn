Version: @wxMathPlot_VERSION_MAJOR@.@wxMathPlot_VERSION_MINOR@.@wxMathPlot_VERSION_BUILD@

Requires:
CXXflags: -I${includedir}
Libs: -L${libdir} -l@MATHPLOT@
